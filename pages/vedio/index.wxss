/**index.wxss**/
.container{

}
.page-section-spacing{
  margin-top: 20rpx;
  font-size:30rpx;
  font-weight: bold;
  height:100rpx;
  line-height:100rpx;
}
.scroll-view_H{
  white-space: nowrap;
}

.scroll-view-item_H{
  display: inline-block;
  margin:0 24rpx;
}
.scroll-view-item_H text{
	border-bottom:1px solid #fff;
	padding-bottom:6rpx;
}


.searchInfo{
	border:1px solid #6699FF;
	display:-webkit-flex;
	display:-moz-flex;
	display: flex;
	flex-wrap:wrap;
	justify-content: flex-start;
	font-size:30rpx;
	line-height:80rpx;
	margin-bottom:10rpx;
	border-radius: 10rpx;
	margin:10rpx 18rpx;

}





.mvWrap{
	padding:0 0rpx;
	width:100%;
	font-size:24rpx;
	text-align: center;
	line-height:40rpx;

}
.mvWrap image{
	width:98%;
	height:400rpx;
}
.jumpPage{
	height:100rpx;
	display:-webkit-flex;
	display:-moz-flex;
	display: flex;
	flex-wrap:wrap;
	justify-content: space-around;
	padding:0 40rpx;
	font-size:30rpx;
	line-height:100rpx;
}

